{
    "plugin": "custom",
    "pluginConfig": {
      "invoke_interval": "1m",
      "timeout": "30s",
      "max_output_length": 1024,
      "concurrency": 1
    },
    "source": "network-custom-plugin-monitor",
    "metricsReporting": false,
    "conditions": [],
    "rules": [
      {
        "type": "permanent",
        "reason": "VolumesNotHealthy",
        "path": "/config/plugin/storageos_volume_health.sh"
      }
    ]
}
